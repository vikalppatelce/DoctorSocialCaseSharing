<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >
    <PreferenceCategory android:title="@string/dashboard" >
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="prefIsSX"
            android:summary="@string/dashboard_summary"
            android:title="@string/sx" >
        </CheckBoxPreference>
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="prefIsIPD"
            android:summary="@string/dashboard_summary"
            android:title="@string/ipd" >
        </CheckBoxPreference>
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="prefIsOPD"
            android:summary="@string/dashboard_summary"
            android:title="@string/opd" >
        </CheckBoxPreference>
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="prefIsRSX"
            android:summary="@string/dashboard_summary"
            android:title="@string/rsx" >
        </CheckBoxPreference>
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="prefIsRIPD"
            android:summary="@string/dashboard_summary"
            android:title="@string/ripd" >
        </CheckBoxPreference>
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="prefIsROPD"
            android:summary="@string/dashboard_summary"
            android:title="@string/ropd" >
        </CheckBoxPreference>
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="prefIsPayment"
            android:summary="@string/dashboard_summary"
            android:title="@string/payment" >
        </CheckBoxPreference>
        <CheckBoxPreference
            android:defaultValue="false"
            android:key="prefIsExpense"
            android:summary="@string/dashboard_summary"
            android:title="@string/expense" >
        </CheckBoxPreference>
    </PreferenceCategory>
</PreferenceScreen>